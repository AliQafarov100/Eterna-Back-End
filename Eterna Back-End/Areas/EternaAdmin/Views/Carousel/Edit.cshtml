@model Carousel;
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>
<form method="post" enctype="multipart/form-data">
    <div>
        
    </div>
    <div>
        <label>Image: </label>
        <input asp-for="Photo" class="form-control" accept="image/*" />
        <span asp-validation-for="Photo" class="text-dandger"></span>
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Edit carousel</button>
    </div>
</form>

@section Scripts{
    <script>
        $(".closeBtn").click(function () {
            this.parentNode.remove();
        })
    </script>
}